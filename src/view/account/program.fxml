            <TableColumn fx:id="col_code" prefWidth="125.0" text="Code" />
        </columns>
      </TableView>
      <JFXTextField fx:id="txt_code" labelFloat="true" layoutX="107.0" layoutY="261.0" promptText="Program Code" />
   </children>
</AnchorPane>
